<h3>Player Search!</h3>

<%= form_tag players_path, method: :get do %>
<%= label_tag(:search, "Which Player are you looking for?") %>
<%= text_field_tag :search, params[:search]  %>
<%= submit_tag('Search') %>
<% end %>

<% @players.each do |player| %>
    <ul>
        <li><%= link_to player.full_name, player_path(player) %></li><br>
    </ul>
<% end %>