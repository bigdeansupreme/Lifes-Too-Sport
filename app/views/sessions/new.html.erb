<h2>Fanatic Login:</h2>



<%= form_tag login_path do  %>
    <label>Username:</label>
    <%= text_field_tag :user_name %>
    <label>Password:</label>
    <%= password_field_tag :password %>
    <label>Password Confirmation</label>
    <%= password_field_tag :password_confirmation %>
    <%= submit_tag "LogIn" %>
 <% end %>



